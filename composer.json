{
    "name": "filippo-toso/controllers-generator",
    "description": "A Laravel Artisan command to automatically generate controllers and tests",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Toso",
            "email": "filippo.toso@creativepark.it"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "^5.5",
        "doctrine/dbal": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "FilippoToso\\ControllersGenerator\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilippoToso\\ControllersGenerator\\ServiceProvider"
            ]
        }
    }
}
